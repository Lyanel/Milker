<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>


<AnchorPane xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vue.MilkOptionController">
   <children>
      <TitledPane fx:id="optionTitle" animated="false" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" text="Option">
         <content>
            <GridPane>
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label fx:id="optionLab1" text="Language" />
                  <Label fx:id="optionLab2" text="Option 2" GridPane.rowIndex="1" />
                  <Label fx:id="optionLab3" text="Option 3" GridPane.rowIndex="2" />
                  <ComboBox fx:id="optionSel1" prefWidth="150.0" GridPane.columnIndex="1" />
                  <Button fx:id="saveBT" mnemonicParsing="false" text="Save" GridPane.rowIndex="3" />
                  <Button fx:id="CancelBT" mnemonicParsing="false" text="Cancel" GridPane.columnIndex="1" GridPane.rowIndex="3" />
               </children>
            </GridPane>
         </content>
      </TitledPane>
   </children>
</AnchorPane>
