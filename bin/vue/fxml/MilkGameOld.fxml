            </GridPane>
         </left>
         <right>
            <GridPane BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <FlowPane fx:id="magicBuildingPan" prefHeight="200.0" prefWidth="200.0" />
                  <FlowPane fx:id="magicWorkerPan" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1" />
                  <FlowPane fx:id="magicSlavePan" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="2" />
                  <FlowPane fx:id="magicAnimalPan" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="3" />
               </children>
            </GridPane>
         </right>
         <bottom>
            <GridPane BorderPane.alignment="CENTER">
              <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <FlowPane fx:id="neutralBuildingPan" prefHeight="200.0" prefWidth="200.0" />
                  <FlowPane fx:id="neutralWorkerPan" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" />
                  <FlowPane fx:id="neutralSlavePan" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2" />
                  <FlowPane fx:id="neutralAnimalPan" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="3" />
               </children>
            </GridPane>
         </bottom>
         <top>
            <GridPane BorderPane.alignment="CENTER">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <FlowPane fx:id="researchPan" prefHeight="200.0" prefWidth="200.0" />
                  <FlowPane fx:id="toolPan" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" />
                  <FlowPane fx:id="upgradePan" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2" />
               </children>
