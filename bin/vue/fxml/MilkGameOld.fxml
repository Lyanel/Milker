<AnchorPane xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vue.MilkGameController">
   <children>
      <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <Circle fx:id="venusClick" fill="DODGERBLUE" radius="100.0" stroke="BLACK" strokeType="INSIDE" BorderPane.alignment="CENTER" />
         </center>
         <left>
            <GridPane BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <FlowPane fx:id="scienceBuildingPan" prefHeight="200.0" prefWidth="200.0" />
                  <FlowPane fx:id="scienceWorkerPan" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1" />
                  <FlowPane fx:id="scienceSlavePan" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="2" />
                  <FlowPane fx:id="scienceAnimalPan" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="3" />
